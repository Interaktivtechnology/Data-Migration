  "scripts": {
    "test": "mocha --compilers js:babel-register --recursive",
    "devreact": "NODE_ENV=production watchify -t [ babelify --presets [ react ] ] src/react-view/app.js -o ./public/javascripts/app.js -v -d ",
    "buildreact": "browserify ./src/react-view/app.js -t [envify --NODE_ENV production]  -t [babelify] | uglifyjs -cm > ./public/javascripts/app.min.js",
    "dev": "nodemon --watch src/app.js --watch src/routes-server/ --exec babel-node ./src/app",
    "prestart": "babel src --recursive --out-dir dist && browserify ./src/react-view/app.js -t [envify --NODE_ENV production]  -t [babelify] | uglifyjs -cm > ./public/javascripts/app.min.js",
    "start": "NODE_ENV=production node dist/bin/www"
  },
