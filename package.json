{
  "name": "data-migration",
  "version": "0.0.1",
  "description": "Data Migration for Salesforce with built in View using ReactJS as V and NodeJS as server",
  "main": "index.js",
  "dependencies": {
    "babel-preset-react": "^6.5.0",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "express-session": "^1.13.0",
    "jade": "~1.11.0",
    "morgan": "^1.6.1",
    "pug": "^2.0.0-alpha8",
    "react": "^15.1.0",
    "react-addons-css-transition-group": "^15.1.0",
    "react-bootstrap": "^0.29.4",
    "react-dom": "^15.1.0",
    "react-router": "^2.4.1",
    "redux": "^3.5.2",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "babelify": "^7.3.0",
    "envify": "^3.4.0",
    "mocha": "^2.5.3",
    "nodemon": "^1.9.2"
  },
  "scripts": {
    "test": "mocha --compilers js:babel-register --recursive",
    "devreact": "watchify -t [ babelify --presets [ react ] ] src/react-view/app.js -o ./public/javascripts/app.js",
    "buildreact": "browserify -t [babelify --presets [ react ]] src/react-view/app.js | uglifyjs -cm > ./public/javascripts/app.min.js",
    "dev": "nodemon --watch src/app.js --watch src/server/ --exec babel-node ./src/app",
    "prestart": "babel src --recursive --out-dir dist",
    "start": "NODE_ENV=production node dist/bin/www"
  },
  "author": "eko.purnomo@icloud.com",
  "license": "ISC"
}
